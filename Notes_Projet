while:
    conditions ? CMP ?

condition : 

expr tEQ expr {asm_add(ASM_CMP, last, next_last,...); $$ = ASM_BNQ;}
expr tNE expr {asm_add(ASM_CMP, last, next_last,...); $$ = ASM_BEQ}
...

condins : 
tIF condition {asm_add($2,... )}

